{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/@openui5/ts-types-esm"
    ],
    "paths": {
      "jwozniczak/io/jsonomodel/*": ["./src/*"]
    }
  },
  "include": ["./src/**/*"]
}
